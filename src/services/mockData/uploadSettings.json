{
  "maxFileSize": 10485760,
  "allowedTypes": [
    "image/jpeg",
    "image/png",
    "image/gif",
    "image/webp",
    "application/pdf",
    "application/msword",
    "application/vnd.openxmlformats-officedocument.wordprocessingml.document",
    "text/plain",
    "text/csv",
    "application/zip",
    "video/mp4",
    "video/quicktime",
    "audio/mp3",
    "audio/wav"
  ],
  "maxConcurrentUploads": 3,
  "autoCompress": true
}